{
  "manifest_version": 2,
  "version": "0.1",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon16.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Text Summary"
